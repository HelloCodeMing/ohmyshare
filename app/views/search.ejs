<!DOCTYPE html>
<html>
	<% include head %>
	<body>
		<% include header %>
		<div class="container">
			<div class="main">
				<div class="search-title">
					<h3>Welcome to oh my share</h3>
				</div>
				<div class="search-result">
					<% if (result.length == 0) {%>
					<p>oops, no result</p>
					<% }else {%>
					<% for (var i in result) {%>
					<div class="panel panel-default">
						<div class="panel-heading">
							<%= result[i].name %>
						</div>
						<div class="panel-body">
							<% for (var j in result[i].keywords) {%>
							<%= result[i].keywords[j] %>
							<% } %> 
							<a class="btn btn-primary pull-right" href="<%= result[i].link %>">Download</a>
						</div>
					</div>
					<% }} %>
				</div>
			</div>
		</div>
	</body>
	<% include foot %>
</html>
